<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="test">

	<typeAlias alias="testDto" type="kr.co.sample.bbs.TestDto" />
	
	<select id="getProductList" parameterClass="testDto" resultClass="testDto">
		<![CDATA[
			SELECT prdNm FROM 
				(SELECT a.*, ROWNUM rn FROM 
					(SELECT prdNm
					 FROM NPRD01MT)
				 a WHERE ROWNUM <= #endNo#) 
			WHERE rn  >= #startNo#
		]]>
	</select>
	
</sqlMap>